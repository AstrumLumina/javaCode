<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">

  <display-name>resp-file</display-name>
  <description>
    resp-file
  </description>

  <servlet>
    <servlet-name>downpicture</servlet-name>
    <servlet-class>com.xuanxie.servlet.RespFile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>downpicture</servlet-name>
    <url-pattern>/downpicture1</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>identifypicture</servlet-name>
    <servlet-class>com.xuanxie.servlet.IdentifyPicture</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>identifypicture</servlet-name>
    <url-pattern>/identifycode</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>redirect</servlet-name>
    <servlet-class>com.xuanxie.servlet.RedirectTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>redirect</servlet-name>
    <url-pattern>/redirect</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>dealrequest</servlet-name>
    <servlet-class>com.xuanxie.servlet.DealRequest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dealrequest</servlet-name>
    <url-pattern>/dealrequest</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>success</servlet-name>
    <jsp-file>/WEB-INF/classes/static/jsp/successed.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>success</servlet-name>
    <url-pattern>/successed</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.xuanxie.servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>


</web-app>